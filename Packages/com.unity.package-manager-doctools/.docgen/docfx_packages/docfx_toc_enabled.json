{
	"metadata": [
		{
			"src": [
				{
					"files": [
						"package/**.csproj"
					],
					"exclude": [
						"**/obj/**",
						"**/bin/**",
						"_site/**"
					]
				}
			],
			"dest": "api",
			"filter": "filter.yml"
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"api/**.yml",
					"api/index.md"
				]
			},
			{
				"files": [
					"manual/**.md",
					"manual/**/toc.yml",
					"changelog/**.md",
					"changelog/**/toc.yml",
					"license/**.md",
					"license/**/toc.yml",
					"toc.yml",
					"*.md"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**",
					"manual/Images/**",
					"logo.svg"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"apidoc/**.md"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"template": [
			"./_exported_templates/packages/"
		],
		"postProcessors": [],
		"noLangKeyword": false,
		"keepFileLink": false,
		"dest": "_site",
        "xrefService": [ 
           "https://xref.docs.microsoft.com/query?uid={uid}" 
         ],
		"globalMetadata": {
			"_appTitle": "DEFAULT_APP_TITLE",
            "_packageVersion": "DEFAULT_PACKAGE_VERSION",
			"_enableSearch": true,
			"_appLogoPath": "logo.svg",
			"_disableToc": false,
			"enableTocForManual": true
		},
        "globalMetadataFiles": ["projectMetadata.json"]
	}
}
